Cell["\<\
Here we specify QRMon workflow that rescales the data, fits a B-spline curve \
to get the trend, and finds the absolute and relative errors (residuals, \
fluctuations) around that trend:\
\>", "Text",
 CellChangeTimes->{{3.8197027065705357`*^9, 3.819702738958137*^9}, {
  3.819703113354012*^9, 
  3.819703171562084*^9}},ExpressionUUID->"5bac5b7b-f6ff-4287-8b85-\
65a3b0a6a91f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"qrObj", "=", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"QRMonUnit", "[", "tsData", "]"}], "\[DoubleLongRightArrow]", 
    "\[IndentingNewLine]", "QRMonEchoDataSummary", "\[DoubleLongRightArrow]", 
    "\[IndentingNewLine]", 
    RowBox[{"QRMonRescale", "[", 
     RowBox[{"Axes", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"False", ",", "True"}], "}"}]}], "]"}], 
    "\[DoubleLongRightArrow]", "\[IndentingNewLine]", "QRMonEchoDataSummary", 
    "\[DoubleLongRightArrow]", "\[IndentingNewLine]", 
    RowBox[{"QRMonQuantileRegression", "[", 
     RowBox[{"16", ",", "0.5"}], "]"}], "\[DoubleLongRightArrow]", 
    "\[IndentingNewLine]", 
    RowBox[{"QRMonSetRegressionFunctionsPlotOptions", "[", 
     RowBox[{"{", 
      RowBox[{"PlotStyle", "->", "Red"}], "}"}], "]"}], 
    "\[DoubleLongRightArrow]", "\[IndentingNewLine]", 
    RowBox[{"QRMonDateListPlot", "[", 
     RowBox[{
      RowBox[{"AspectRatio", "\[Rule]", 
       RowBox[{"1", "/", "4"}]}], ",", 
      RowBox[{"ImageSize", "\[Rule]", "Large"}]}], "]"}], 
    "\[DoubleLongRightArrow]", "\[IndentingNewLine]", 
    RowBox[{"QRMonErrorPlots", "[", 
     RowBox[{
      RowBox[{"\"\<RelativeErrors\>\"", "\[Rule]", "False"}], ",", 
      RowBox[{"AspectRatio", "\[Rule]", 
       RowBox[{"1", "/", "4"}]}], ",", 
      RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
      RowBox[{"DateListPlot", "\[Rule]", "True"}]}], "]"}], 
    "\[DoubleLongRightArrow]", "\[IndentingNewLine]", 
    RowBox[{"QRMonErrorPlots", "[", 
     RowBox[{
      RowBox[{"\"\<RelativeErrors\>\"", "\[Rule]", "True"}], ",", 
      RowBox[{"AspectRatio", "\[Rule]", 
       RowBox[{"1", "/", "4"}]}], ",", 
      RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
